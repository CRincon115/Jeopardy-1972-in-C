import turtle

grid = turtle.Turtle()

grid.forward(200)
grid.left(90)
grid.forward(200)
grid.left(90)
grid.forward(80)
grid.left(90)
grid.forward(400)
grid.left(90)
grid.forward(80)
grid.left(90)
grid.forward(100)
grid.left(90)
grid.forward(400)
grid.backward(400)
grid.right(90)
grid.forward(100)
grid.left(90)
grid.forward(400)
grid.backward(400)
grid.right(90)
grid.forward(100)
grid.left(90)
grid.forward(400)
grid.backward(400)
grid.right(90)
grid.forward(100)
grid.left(90)
grid.forward(400)
grid.backward(240)
grid.left(90)
grid.forward(400)
grid.left(90)
grid.forward(80)
grid.backward(320)
grid.forward(160)
grid.left(90)
grid.forward(400)
grid.left(90)
grid.forward(80)
grid.left(90)
grid.forward(400)
grid.right(90)
grid.forward(80)
grid.right(90)
grid.forward(400)
grid.backward(20)
grid.right(90)
grid.forward(400)



